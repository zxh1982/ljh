<%- include('head'); -%>

<table class="table table-striped table-responsive table-bordered table-hover">
    <caption> <%= title %> </caption>

    <thead>
        <tr>
            <th>姓名</th>
            <th>会员卡号</th>
            <th>电话</th>
            <th>生日</th>
        </tr>
    </thead>
    <tbody>
        <% memberList.forEach(function(member){ %>
            <tr class="">
                <td> <%= member.name %> </td>
                <td> <%= member.memberNum %> </td>
                <td> <%= member.phoneNum %> </td>
                <td> <%= member.birthday %> </td>
            </tr>
        <% }); %>
    </tbody>
</table>
